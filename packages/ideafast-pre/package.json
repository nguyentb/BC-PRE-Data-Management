{
    "name": "ideafast-pre",
    "version": "0.0.1",
    "description": "An efficient pairing free PRE.",
    "contributors": [
        {
            "name": "Nguyen Truong",
            "email": "n.truong@imperial.ac.uk",
            "url": "https://github.com/nguyentb"
        },
        {
            "email": "f.guitton@imperial.ac.uk",
            "name": "Florian Guitton",
            "url": "https://github.com/fguitton"
        }
    ],
    "main": "dist/index.js",
