        "@babel/preset-env": "7.12.1",
        "arraybuffer-loader": "1.0.8",
        "babel-loader": "8.1.0",
        "babel-jest": "26.6.1",
        "cross-env": "7.0.2",
        "jest": "26.6.1",
        "webpack": "4.44.2",
        "webpack-nano": "1.0.0",
        "webpack-node-externals": "2.5.2"
    },
    "jest": {
        "transform": {
            "^.+\\.(js|html|scss)$": "<rootDir>/test/preprocessor.js"
        },
        "testMatch": [
            "**/+(*.)(spec|test).{js,jsx,mjs}"
        ]
